<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Clike">
	<select id="getCategory" resultType="Category">
		select cno||',' cno, main, sub from category order by cno
	</select>
	<select id="getClike" parameterType="String" resultType="Clike">
		select * from clike where mid=#{mid}
	</select>
	<insert id="insertClike" parameterType="Clike">
		insert into clike values (clike_sq.nextval, #{mid},#{cno})
	</insert>
</mapper>